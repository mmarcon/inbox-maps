{
  "name": "Inbox Maps",
  "description": "Add maps to your emails",
  "version": "0.1",
  "web_accessible_resources": [
    "assets/*.png"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://mail.google.com/*",
        "https://inbox.google.com/*"
      ],
      "js": ["inboxsdk.js","content.js"],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "https://mail.google.com/",
    "https://inbox.google.com/"
  ],
  "manifest_version": 2
}
